#include <iostream>
using namespace std;

int main()
{
	int N(0);
	cin >> N;
	int *arr = new int [N];
	int *arrReal = new int[N];

	int number(0);
	for (int i(0); i < N; ++i)
	{
		cin >> number;

		if (number > 0)
			arr[i] = 1;
		else
			arr[i] = 0;

		arrReal[i] = number;
	}

	int index(0);
	for (int i(0); i < N; ++i)
	{
		if (arr[i] == 0)
			index = i;
	}
	index++;
	
	int negative(0), positive(0);
	for (int i(0); i < index; ++i)
	{
		if (arrReal[i] >= 0)
			positive += arrReal[i];
		else
			negative += arrReal[i];
	}

	int result = negative + positive;

	if (result < 0)
	{
		result = result * -1;
		cout << result;
	}
	else
		cout << 0;

	return 0;
}
